{
    "manifest_version": 3,
    "name": "Candidate-Finder",
    "version": "1.0",
    "description": "Parse candidate LinkedIn data and send it to API after login.",
    "permissions": [
      "storage",
      "scripting",
      "tabs",
      "activeTab",
      "webNavigation"  
    ],
    "host_permissions": [
      "http://localhost:3000/*",
      "https://www.linkedin.com/*"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "auth/auth.html",
      "default_icon": {
        "16": "icons/icon16.png"
      }
    }
  }
  